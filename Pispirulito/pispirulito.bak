program pispirulito;
uses crt;
var
    c:real;
const
    p = 179;
    r = 205;
    e = 98;
    g = 180;
    l = 199;
begin
    clrscr;
    writeln('Programa para ayudar a Pispirulito con la cuenta');
    writeln('Programado por Rafael Vela');
    writeln(' ');
    c:= p+r+e+g+l;
    writeln('Pispirulito y cada uno de sus cuates pagara: $',c:4:2);
    readkey;
end.
